{
  "$schema": "https://www.canva.dev/schemas/app/v1/manifest-schema.json",
  "manifest_schema_version": 1,
  "runtime": {
    "permissions": [
      {
        "name": "canva:design:content:read",
        "type": "mandatory"
      },
      {
        "name": "canva:design:content:write",
        "type": "mandatory"
      }
    ],
    "external_oauth": {
      "provider": "contentful",
      "authorization_url": "https://be.contentful.com/oauth/authorize",
      "token_url": "https://be.contentful.com/oauth/token",
      "scopes": ["content_management_manage"]
    }
  },
  "intent": {
    "data_connector": {
      "enrolled": true
    }
  }
}
